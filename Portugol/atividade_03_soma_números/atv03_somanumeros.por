programa {
  funcao inicio() {

    // Declaração de variáveis
    inteiro numeroUm
    inteiro numeroDois

    // Entrada de Dados
    escreva("Digite um número: ")
    leia(numeroUm)

    escreva("Digite outro número: ")
    leia(numeroDois)

    // Saída de dados
    escreva(numeroUm + numeroDois)

  }
}
